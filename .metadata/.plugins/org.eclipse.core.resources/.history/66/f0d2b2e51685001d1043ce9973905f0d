
public class Thread2 implements Runnable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread2 obj=new Thread2();
		Thread t=new Thread(obj);
		
		t.run();
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		System.out.println("Thread is running");
	}

}
